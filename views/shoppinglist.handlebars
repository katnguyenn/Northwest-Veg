<h1 class="h1reg center">Shopping List</h1>
<section class="container center" >
    
    <div class="container-recipe">
        {{#each result}}
        <div class="row">
            <div class="col s12">
                <main id="main">
                    <div class="recipe">
                        <section>
                            <img class="home-image" src={{recipe.image}} alt="recipe image" />
                            <div class="recipe-info">
                                <h3 style="font-size:20px">{{recipe.label}}</h3>
                                <span class="veggie">VEG</span>
                            </div>
                            <div class="overview">
                                <a href={{recipe.url}}>Click here for the Recipe!</a>
                            </div>
                        </section>
                    </div>
                </main>
            </div>
        </div>
        {{/each}}
    </div>
</section>